{
  "name": "merodost",
  "version": "1.0.0",
  "description": "MeroDost is a bloging website",
  "main": "index.js",
  "engines": {
    "node": "12.14.1",
    "npm": "6.13.4"
  },
  "scripts": {
    "client": "cd client && npm start",
    "install-client": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run install-client && npm run build",
    "start": "npm run install --prefix server && npm run start --prefix server",
    "dev": "concurrently \"npm run start\" \"npm run client\""
  },
  "author": "Bibek Shrestha",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^5.2.0"
  }
}
